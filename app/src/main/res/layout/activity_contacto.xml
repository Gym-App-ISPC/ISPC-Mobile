<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.activities.ContactoActivity">


    <Button
        android:id="@+id/btnObBack"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="329dp"
        android:layout_marginBottom="700dp"
        android:background="@drawable/baseline_arrow_circle_left_24"
        android:clickable="true"
        android:contentDescription="@string/arrow_return_description"
        android:onClick="returnToHome"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/contactanos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="114dp"
        android:layout_marginBottom="18dp"
        android:text="@string/contactanosTXT"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/nombreYApellido"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="68dp"

        android:contentDescription="TODO"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email"
        app:layout_constraintVertical_bias="0.908"
        app:srcCompat="@drawable/btncontacto"
        tools:ignore="ContentDescription,HardcodedText" />

    <EditText
        android:id="@+id/email"
        android:layout_width="330dp"
        android:layout_height="76dp"
        android:layout_marginHorizontal="40dp"
        android:layout_marginBottom="457dp"
        android:autofillHints="true"
        android:drawablePadding="10dp"
        android:hint="@string/email"
        android:inputType="textEmailAddress"
        android:paddingBottom="39dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.253"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contactanos"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/nombreYApellido"
        android:layout_width="331dp"
        android:layout_height="48dp"
        android:layout_marginHorizontal="40dp"
        android:layout_marginBottom="533dp"
        android:autofillHints="true"
        android:drawablePadding="10dp"
        android:hint="@string/nombreYApellido"
        android:inputType="text"
        android:paddingBottom="39dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.258"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contactanos" />

    <EditText
        android:id="@+id/escribaSuMensajeAqui"
        android:layout_width="324dp"
        android:layout_height="146dp"
        android:layout_marginHorizontal="40dp"
        android:layout_marginBottom="116dp"
        android:autofillHints="true"
        android:drawablePadding="10dp"
        android:hint="@string/escribaSuMensajeAqui"
        android:inputType="text"
        android:paddingBottom="110dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.782"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>